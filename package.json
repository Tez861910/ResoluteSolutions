{
  "name": "resolute-solutions",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://tez861910.github.io/ResoluteSolutions",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:debug": "vite --host 0.0.0.0 --debug",
    "dev:force": "vite --host 0.0.0.0 --force",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview --host 0.0.0.0",
    "preview:dist": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules package-lock.json && npm install",
    "serve": "npm run build && npm run preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.0",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-plugin-obfuscator": "^1.0.5"
  }
}